用循环语句来求一到一百的奇数
#include<stdio.h>
int main()
{
int i;
for(i=1;i<=100;i++)
{
if(i%2==1)
printf("%d\n");
}
return 0;
}


#include<stdio.h>
int main()
{
int i=1;
while(i<=100)
{
if(i%2==1)
printf("%d\n");
i++;
}
return 0;
}

for循环语句                                           
#include<stdio.h>                                                                
int main()
{
	int i=0;
	for(i=1;i<=10;i++)
	{
		if(i==5)
	    continue;
		printf("%d\n",i);//最后输出1，2，3，4，6，7，8，9，10
	}
	return 0;
 }


#include<stdio.h>
int main()
{
	int i=0;
	while(i<=10)
	{
		if(i==5)
		continue;
		printf("%d\n",i)；//最后输出1，2，3，4后不会出现5，陷入死循环。
                i++;
	}
	return 0;
}


#include<stdio.h>
int main()
{
	int i=0;
	for(i=1;i<=10;i++)
	{
		if(i==5)
	    break;
		printf("%d\n",i);最后输出1，2，3，4
	}
	return 0;
 } 
